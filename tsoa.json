    "host": "localhost:8200",
    "basePath": "/"
  },
  "info": {
    "description": "TypeScript Nodejs Boilerplate for Rest Services",
    "version": "1.0.0",
    "title": "TNB"
  },
  "schemes": [
    "http",
    "https"
  ],
